.wrap{
  width:100%;
  position: fixed;
  top:160rpx;
  bottom:0;
  overflow: hidden;
  padding-top:20rpx;
}
.time{
  width:90%;
  margin:0 auto;
  height:80rpx;
  line-height: 80rpx;
  color:#444;
  font-weight: 600;
  text-align: center;
  /* border-bottom: 1px solid #444; */
}
.add_input{
  width:90%;
  display: block;
  margin:0 auto;
  height:80rpx;
  line-height: 80rpx;
  text-align: center;
  border:1px dotted #444;
  box-sizing: border-box;
}
.scroll_box{
  height:100%;
  width:90%;
  margin:0 auto;
  overflow: hidden;
}
.list_wrap{
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  margin-bottom:20rpx;
  box-sizing: border-box;
  display: flex;
  background:#fff;
  color:#333;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.label{
  width:30rpx;
  height:30rpx;
  border:1px solid #999;
  border-radius: 3px;
}
.ctn{
  width:75%;
}
.remove image{
  width:36rpx;
  height:36rpx;
}